
# PostgreSQL configuration
PG_PASS=example_YOUR_SECURE_PASSWORD_HERE  # Replace with the actual database YOUR_SECURE_PASSWORD_HERE
PG_USER=example_user  # Replace with the actual database user
PG_DB=example_db  # Replace with the actual database name

# Redis configuration
REDIS_PASSWORD=example_redis_YOUR_SECURE_PASSWORD_HERE  # Replace with the actual Redis YOUR_SECURE_PASSWORD_HERE if needed

# Authentik configuration
AUTHENTIK_IMAGE=ghcr.io/goauthentik/server  # Replace with the actual Authentik image if different
AUTHENTIK_TAG=latest  # Replace with the actual Authentik tag if different
AUTHENTIK_SECRET_KEY=example_YOUR_APP_SECRET_KEY_HERE  # Replace with a secure secret key

# Traefik configuration
TRAEFIK_DOMAIN=example.com  # Replace with your domain name
TRAEFIK_LOCAL_DOMAIN=local.example.com  # Replace with your local domain name

# Timezone configuration
TZ=America/New_York  # Replace with your timezone